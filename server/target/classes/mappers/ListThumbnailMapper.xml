<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.SOOKTUBE.dao.ListThumbnailDAO">

    <insert id="newThumbnail"
            parameterType="com.SOOKTUBE.model.ListThumbnailDTO"
            useGeneratedKeys="true"
            keyProperty="thumbID">
        INSERT INTO listThumbnail(`listID`, `thumbFileName`)
        VALUES (#{listID}, #{thumbFileName})
    </insert>


</mapper>